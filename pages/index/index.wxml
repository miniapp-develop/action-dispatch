<view class="container">
    <view class="sample_card"
          data-url="/pages/pageB/index?from=source1"
          bind:tap="onTap">
        点击打开其他页面(path方式)
    </view>
    <view class="sample_card"
          data-url="mini://page?_name=pageB&from=source2"
          bind:tap="onTap">
        点击打开其他页面(name方式)
    </view>
    <view class="sample_card"
          data-url="mini://miniapp?appId=xxxxxx&path=%2Fpages%2FpageB%2Findex%3Ffrom%3Dsource3"
          bind:tap="onTap">
        点击打开其他小程序
    </view>
    <view class="sample_card"
          data-url="mini://webview?_url=https%3A%2F%2Faa.bb.cc%2Fxyz%3Fkey%3Dvalue&title=this_is_title&color=red"
          bind:tap="onTap">
        点击打开网页(webview方式)
    </view>
    <view class="sample_card"
          data-url="https://aa.bb.cc/xyz?key=value&from=source5" bind:tap="onTap">
        点击打开网页(http方式)
    </view>
    <view class="sample_card"
          data-url="mini://dialog?title=Demo&content=Hello" bind:tap="onTap">
        点击弹出对话框(自定义)
    </view>
    <view class="sample_card"
          data-url="mini://function/aFn?title=Demo&content=Hello" bind:tap="onTap">
        点击调用页面内方法
    </view>
</view>
